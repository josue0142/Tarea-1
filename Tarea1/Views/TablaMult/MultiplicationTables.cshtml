@{
    ViewData["Title"] = "Tablas de Multiplicar";
}

<h1 style="text-align:center">Tablas de Multiplicar</h1>

<div class="col-12" style="margin-left:auto; margin-right:auto">
    <table border="1">
        @for (int i = 1; i <= 12; i++)
        {
            <tr>
                @for (int j = 1; j <= 12; j++)
                {
                    <td>@i * @j = @(i * j)</td>
                }
            </tr>
        }
    </table>
</div>

